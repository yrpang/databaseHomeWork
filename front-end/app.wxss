/**app.wxss**/
Page {
  background-color: rgb(245, 245, 245);
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

.center-box {
  display: flex;
  width: 100%;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
  position: relative;
  margin-top: 100rpx;
  margin-bottom: 50rpx;
}

.title{
  font-family: PingFangSC-Bold;
  font-size: 40rpx;
  color: #000;
  text-align: center;
  font-weight: bolder;
}